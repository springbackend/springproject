<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="u">
	<select id="select_list" resultType="user">
		SELECT * FROM USER1
	</select>
	<select id="select_one" resultType="user" parameterType="String">
		SELECT *
		FROM USER1
		WHERE u_id = '${value}'
	</select>
	<insert id="insert_user" parameterType="user">
		INSERT INTO MEMBER VALUES(
			u_idx = user_seq.nextVal,
			u_id = id,
			u_pwd = pwd,
			u_name = name,
			u_pnum = pnum,
			u_regidate = regidate,
			u_regiip = regiip,
			u_gender = gender,
			u_email = email
		)
	</insert>
</mapper>











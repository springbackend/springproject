<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="t">
	<select id="select_tone" resultType="String" parameterType="int">
		SELECT t_name FROM tone
		WHERE t_idx = #{t_idx}
	</select>

</mapper>













<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bp">
	<select id="select_product_name" resultType="String" parameterType="int">
		SELECT p_name FROM board_product
		WHERE p_idx = #{p_idx}
	</select>

</mapper>













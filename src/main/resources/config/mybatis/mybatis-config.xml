<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>

	<typeAliases>
		<typeAlias type="vo.UserVO" alias="user" />
		<typeAlias type="vo.ProductVO" alias="product" />
		<typeAlias type="vo.BoardVO" alias="board" />
		<typeAlias type="vo.CommentVO" alias="comment" />
		<typeAlias type="vo.LikesVO" alias="likes" />
		<typeAlias type="dao.AdminDTO" alias="admin" />
		<typeAlias type="vo.ProductCommentVO" alias="productcomment"/>
		<typeAlias type="vo.PcGoodVO" alias="pcgood"/>
		<typeAlias type="vo.CartVO" alias="cart"/>
	</typeAliases>

	<mappers>
		<!-- <mapper resource="config/mybatis/mapper/visit.xml" /> -->
		<mapper resource="config/mybatis/mapper/user.xml" />
		<mapper resource="config/mybatis/mapper/product.xml" />
		<mapper resource="config/mybatis/mapper/board.xml"/>
		<mapper resource="config/mybatis/mapper/comment.xml"/>
		<mapper resource="config/mybatis/mapper/tone.xml"/>
		<mapper resource="config/mybatis/mapper/likes.xml"/>
		<mapper resource="config/mybatis/mapper/search.xml"/>
		<mapper resource="config/mybatis/mapper/productComment.xml"/>
		<mapper resource="config/mybatis/mapper/userBoard.xml" />
		<mapper resource="config/mybatis/mapper/pcGood.xml" />
		<mapper resource="config/mybatis/mapper/cart.xml" />
	</mappers>
</configuration>
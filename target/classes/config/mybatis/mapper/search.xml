<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="s">
	<select id="search_p_name" resultType="product" parameterType="java.util.Map">
			SELECT * FROM product WHERE p_name BETWEEN #{one} AND #{two}
	</select>
	<select id="search_p_namet" resultType="product" parameterType="String">
			SELECT * FROM product WHERE p_name like '%' || #{keyword} || '%'
	</select>
	
</mapper>





















